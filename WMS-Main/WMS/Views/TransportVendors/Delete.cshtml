@model WareHouseMVC.Models.TransportVendor

@{
    ViewBag.Title = "Delete";
}
<div class="pagetitle">
    <h1>
      Delete Transport Vendor </h1>
    <span>Are you sure you want to delete this?</span>
</div>

@using (Html.BeginForm()) {
<table class="table table-bordered table-invoice">
    <tbody>

  <tr>
        <td class="width30">Name</td>
        <td>
        @Model.Name
        </td>
        </tr>

    

  <tr>
        <td class="width30">Code</td>
        <td>
        @Model.Code
        </td>
        </tr>

    

  <tr>
        <td class="width30">ContactNo</td>
        <td>
        @Model.ContactNo
        </td>
        </tr>

    

  <tr>
        <td class="width30">Vehicles</td>
        <td>
        @(Model.Vehicles == null ? "None" : Model.Vehicles.Count.ToString())
        </td>
        </tr>

    

 <tr>
         <td class="width30"></td>
        <td>
    <input type="submit" value="Delete" class="btn btn-danger" /> 
        </td>
        </tr>
		  </tbody>
        </table>
    <p>
       
      @Html.ActionLink("Back to List", "Index", "", new { @class = "btn" })
  
    </p>
}


