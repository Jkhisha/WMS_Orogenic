@model WareHouseMVC.Models.LoginTrail

@{
    ViewBag.Title = "Delete";
}
<div class="pagetitle">
    <h1>
      Delete  </h1>
    <span>Are you sure you want to delete this?</span>
</div>






@using (Html.BeginForm()) {
<table class="table table-bordered table-invoice">
    <tbody>

  <tr>
        <td class="width30">LoginBy</td>
        <td>
        @Model.LoginBy
        </td>
        </tr>

    

  <tr>
        <td class="width30">LoginDate</td>
        <td>
        @String.Format("{0:g}", Model.LoginDate)
