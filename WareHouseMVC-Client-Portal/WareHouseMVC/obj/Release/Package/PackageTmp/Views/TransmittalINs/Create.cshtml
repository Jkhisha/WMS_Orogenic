@model WareHouseMVC.Models.TransmittalIN

@{
    ViewBag.Title = "Tansmittal IN";
}

<script type="text/javascript">
    jQuery(function ($) {
        $('input[type="submit"]').click(function (e) {
            if ($("select#DepartmentID").val() != "" && $("input[name='ContactPersontID']").val() == "") {
                e.preventDefault();
                alert("The department does not have a 'Contact Person'. Please reach out to the admin.");
                return;
            }
        });
    });
</script>



<div class="pagetitle">
    <h1>
       Create Transmittal IN </h1>
    <span>Creates Transmittal IN's below....</span>
</div>


@using (Html.BeginForm("Create","TransmittalINs",FormMethod.Post,new { enctype = "multipart/form-data" })) {
    @Html.ValidationSummary(true)
	
		@Html.Partial("_CreateOrEdit", Model)
    <table class="table table-bordered table-invoice">
