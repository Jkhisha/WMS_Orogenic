@model IEnumerable<WareHouseMVC.Models.Level>

@{
    ViewBag.Title = "Levels";
}

<div class="pagetitle">
    <h1>
      Level List</h1>
    <span>List of Levels....</span>
</div>
<p>
    @Html.ActionLink("Create New", "Create", "", new { @class = "btn btn-primary" })
</p>

<div id="dyntable_wrapper" class="dataTables_wrapper" role="grid">
 <table class="table table-bordered" id="dyntable">
  <thead>
    <tr>
        <th></th>
        <th>
            LevelName
        </th>
        <th>
            LevelCode
        </th>
        <th>
            Rack
        </th>
        <th>
            Train
        </th>
        <th>
            Zone
        </th>
        <th>
            Floor
        </th>
        <th>
            Warehouse
        </th>
    </tr>
	 </thead>
	  <tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(" ", "Edit", new { id=item.LevelID }, new { @class = "icon-edit" }) |
            @Html.ActionLink(" ", "Details", new { id=item.LevelID }, new { @class = "icon-list-alt" }) |
            @Html.ActionLink(" ", "Delete", new { id=item.LevelID }, new { @class = "icon-trash" }) |
              @Html.ActionLink("Add Height ", "CreateWithLV", "Heights", new { lvid = item.LevelID }, "")
        </td>
        <td>
			@item.LevelName
        </td>
        <td>
			@item.LevelCode
        </td>
        <td>
			@Html.DisplayTextFor(_ => item.Rack.RackName).ToString()
        </td>
        <td>
			@Html.DisplayTextFor(_ => item.Train.TrainName).ToString()
        </td>
        <td>
			@Html.DisplayTextFor(_ => item.Zone.ZoneName).ToString()
        </td>
        <td>
			@Html.DisplayTextFor(_ => item.Floor.FloorName).ToString()
        </td>
        <td>
			@Html.DisplayTextFor(_ => item.Warehouse.WarehouseName).ToString()
        </td>
    </tr>
}
 </tbody>

</table>

</div>
